<!-- ********************************** -->	

<!-- DAY SPAWNING OUTSIDE WILDERNESS -->
<append xpath="/entitygroups/entitygroup[@name='PineforestZombiesDay']">
	<entity name="SwatCaptainZombie" prob="0.10"/>
	<entity name="Parasite" prob="0.10"/>
	<entity name="BloatedParasite" prob="0.10"/>
</append>


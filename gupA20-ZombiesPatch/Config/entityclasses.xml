<configs>

<!-- ********** ZOMBIE TWEAKS ********** -->
<!-- The Guppy zombies were initially set to generic values for Health, Exp gain, physical damage resist, attack strength, etc. Below are some modifications to vary these charactertics to make more sense with their physical looks and follow enZombie patterns. Female zombies voices were also set to male, so now they use female voices.
-->

<!-- zombieGuppyAlma -->
<set xpath="/entity_classes/entity_class[@name='zombieGuppyAlma']/property[@name='ExperienceGain']/@value">330</set>
<set xpath="/entity_classes/entity_class[@name='zombieGuppyAlma']/property[@name='LootDropProb']/@value">0.02</set>
<set xpath="/entity_classes/entity_class[@name='zombieGuppyAlma']/property[@name='SizeScale']/@value">1.1</set>
<set xpath="/entity_classes/entity_class[@name='zombieGuppyAlma']/effect_group/passive_effect[@name='PhysicalDamageResist']/@value">0</set>
<set xpath="/entity_classes/entity_class[@name='zombieGuppyAlma']/effect_group/passive_effect[@name='HealthMax' and @operation='base_set']/@value">120</set>
<remove xpath="/entity_classes/entity_class[@name='zombieGuppyAlma']/effect_group/passive_effect[@name='HealthMax' and @operation='perc_set']"/>
<append xpath="/entity_classes/entity_class[@name='zombieGuppyAlma']">
	<property name="SoundRandom" value="Enemies/Base_Zombie_Female/zombiefemaleroam"/>
	<property name="SoundAlert" value="Enemies/Base_Zombie_Female/zombiefemalealert"/>
	<property name="SoundSense" value="Enemies/Base_Zombie_Female/zombiefemalesense"/>
	<property name="SoundHurt" value="Enemies/Base_Zombie_Female/zombiefemalepain"/>
